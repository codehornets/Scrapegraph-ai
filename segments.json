    "Intent-Based": [
        {
            "name": "Informational Search Intent",
            "field": "intent",
            "operator": "==",
            "value": "informational",
            "segment_name": "informational",
            "priority": 1,
            "sub_segments": [
                {
                    "name": "Researching",
                    "field": "intent",
                    "operator": "==",
                    "value": "researching",
                    "segment_name": "researching",
                    "priority": 1,
                    "conditions": {
                        "query_contains": [
                            "how",
                            "what",
                            "guide",
                            "overview"
                        ],
                        "intent": "Informational"
                    }
                },
                {
                    "name": "Evaluating",
                    "field": "intent",
                    "operator": "==",
                    "value": "evaluating",
                    "segment_name": "evaluating",
                    "priority": 2,
                    "conditions": {
                        "query_contains": [
                            "compare",
                            "best",
                            "alternatives"
                        ],
                        "intent": "Informational"
                    }
                }
            ]
        },
        {
            "name": "Transactional Search Intent",
            "field": "intent",
            "operator": "==",
            "value": "transactional",
            "segment_name": "transactional",
            "priority": 2
        },
        {
            "name": "SEO Optimized Leads",
            "field": "seo_score",
